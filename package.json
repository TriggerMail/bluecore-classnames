{
  "name": "bluecore-classnames",
  "version": "0.3.6",
  "description": "Automatic class builder for React components",
  "keywords": [
    "react",
    "classnames",
    "cx",
    "classes",
    "bem"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/jest-cli/bin/jest.js",
    "lint": "./node_modules/coffeelint/bin/coffeelint -r -f coffeelint.json lib/",
    "build": "./node_modules/webpack/bin/webpack.js --progress --colors",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TriggerMail/bluecore-classnames.git"
  },
  "author": "Pavel Vlasov <freakycue@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TriggerMail/bluecore-classnames/issues"
  },
  "homepage": "https://github.com/TriggerMail/bluecore-classnames",
  "dependencies": {
    "underscore": "^1.8.3"
  },
  "peerDependencies": {
    "react": ">=0.14.0"
  },
  "devDependencies": {
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.14.1",
    "jest-cli": "^0.8.2",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "webpack": "^1.12.9"
  },
  "jest": {
    "scriptPreprocessor": "./jest-preprocessor.js",
    "testFileExtensions": [
      "coffee"
    ],
    "moduleFileExtensions": [
      "coffee",
      "js"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
